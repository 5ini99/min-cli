{
  "compilers": {
    "babel": {
      "sourceMaps": "inline",
      "presets": [
        "env"
      ],
      "plugins": [
        "syntax-export-extensions",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-export-extensions"
      ]
    }
  },
  "plugins": {
    "imagemin": {
      "jpg": {},
      "png": {
        "quality": "1"
      }
    },
    "filesync": {
      "cwd": "src/assets",
      "from": ["**/*.jpg", "**/*.png"],
      "to": "/test"
    }
  },
  "lint": {
    "eslint": false
  }
}
