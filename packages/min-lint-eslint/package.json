{
  "name": "@mindev/min-lint-eslint",
  "version": "1.0.0",
  "description": "min eslint",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/meili/min/tree/master/packages/min-lint-eslint"
  },
  "scripts": {
    "clean": "rm -rf ./{dist,lib,types}",
    "build": "../../node_modules/.bin/tsc",
    "watch": "../../node_modules/.bin/tsc -w",
    "lint": "../../node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "min",
    "lint",
    "eslint"
  ],
  "dependencies": {
    "eslint": ">=1.6.0 <4.0.0",
    "eslint-friendly-formatter": ">=2.0.0 < 3.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.0"
  },
  "author": "Min Team",
  "license": "MIT"
}
