{
  "name": "@minlib/minx",
  "version": "2.0.0",
  "description": "state management for min",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "clean": "rm -rf ./{dist,lib,types}",
    "build": "../../node_modules/.bin/rollup --config ./rollup.config.js",
    "watch": "../../node_modules/.bin/rollup --config ./rollup.config.js --watch",
    "lint": "../../node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "prepublish": "npm run clean && npm run build && npm run dist",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "author": "Min Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "22.2.3",
    "chalk": "2.4.1",
    "jest": "22.4.3",
    "ts-jest": "22.4.4"
  },
  "dependencies": {}
}
